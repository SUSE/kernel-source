@@ -94,6 +94,11 @@ static const struct dmi_system_id dmi_ppag_approved_list[] = {
 			DMI_MATCH(DMI_SYS_VENDOR, "Razer"),
 		},
 	},
+	{ .ident = "Honor",
+	  .matches = {
