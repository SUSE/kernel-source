---
 drivers/gpu/drm/amd/amdgpu/gmc_v12_0.c | 5 +++++
 1 file changed, 5 insertions(+)

diff --git a/drivers/gpu/drm/amd/amdgpu/gmc_v12_0.c b/drivers/gpu/drm/amd/amdgpu/gmc_v12_0.c
index 00b7aef6bdb0..e2c6ec3cc4f3 100644
