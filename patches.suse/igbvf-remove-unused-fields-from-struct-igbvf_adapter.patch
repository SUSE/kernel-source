@@ -2707,7 +2703,6 @@ static int igbvf_probe(struct pci_dev *p
 	struct igbvf_adapter *adapter;
 	struct e1000_hw *hw;
 	const struct igbvf_info *ei = igbvf_info_tbl[ent->driver_data];
