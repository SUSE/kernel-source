+++ b/drivers/nvme/host/pci.c
@@ -907,7 +907,7 @@ static blk_status_t nvme_prep_rq(struct nvme_dev *dev, struct request *req)
 			goto out_unmap_data;
 	}
 
-	blk_mq_start_request(req);
+	nvme_start_request(req);
 	nvme_submit_cmd(nvmeq, cmnd, bd->last);
