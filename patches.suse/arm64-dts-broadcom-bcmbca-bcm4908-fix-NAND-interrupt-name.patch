+++ b/arch/arm64/boot/dts/broadcom/bcmbca/bcm4908.dtsi
@@ -449,7 +449,7 @@
 			reg = <0x1800 0x600>, <0x2000 0x10>;
 			reg-names = "nand", "nand-int-base";
